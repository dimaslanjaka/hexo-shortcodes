{
  "name": "hexo-shortcodes-test",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run server",
    "build": "clear && hexo generate",
    "clean": "clear && hexo clean && rimraf tmp",
    "deploy": "hexo deploy",
    "server": "clear && hexo server -p 4000",
    "serve-static": "php -S localhost:4000 -t public/",
    "update:ncu": "npm-check-updates -u -x chalk"
  },
  "hexo": {
    "version": "7.3.0"
  },
  "engines": {
    "node": ">=18"
  },
  "workspaces": [
    "themes/*"
  ],
  "dependencies": {
    "hexo": "^7.3.0",
    "hexo-asset-link": "^2.2.3",
    "hexo-browsersync": "github:hexojs/hexo-browsersync",
    "hexo-cli": "^4.3.2",
    "hexo-front-matter": "^4.2.1",
    "hexo-renderer-marked": "^6.3.0",
    "hexo-server": "^3.0.0",
    "hexo-shortcodes": "workspace:^",
    "hexo-theme-flowbite": "workspace:^"
  },
  "devDependencies": {
    "@types/stylus": "^0",
    "binary-collections": "^2.0.6",
    "cross-env": "^7.0.3",
    "cross-spawn": "https://github.com/dimaslanjaka/node-cross-spawn/raw/5560156/release/cross-spawn.tgz",
    "js-yaml": "^4.1.0",
    "moment": "^2.30.1",
    "prompt": "^1.3.0",
    "rimraf": "^6.0.1",
    "yaml": "^2.6.0"
  },
  "description": "page builder for `hexo-shortcodes`",
  "main": "postinstall.js",
  "keywords": [
    "hexo"
  ],
  "author": {
    "name": "Dimas Lanjaka",
    "email": "dimaslanjaka@gmail.com",
    "url": "https://www.webmanajemen.com"
  },
  "license": "MIT"
}
